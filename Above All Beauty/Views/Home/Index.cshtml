@{
    ViewBag.Title = "Home | Above All Beauty Pageant";
}

<div class="jumbotron" id="home-jumbotron">
    <h1>
        Welcome to Above All Beauty Pageant's Website
    </h1>

    <h4>
        Find out about general information, contact information, and upcoming events!
    </h4>
</div>

<div id="home-flex-box">

    <div id="home-slideshow-flex-item">
        <div id="home-slide-show" class="carousel slide carousel-fade" data-ride="carousel">
            @Html.Partial("~/Views/Shared/_HomeSlideShow.cshtml")
        </div>
    </div>

    <div id="home-flex-item">
        @Html.Partial("~/Views/Shared/_Testimonals.cshtml")
    </div>

</div>


<div id="upcoming">
    <h1 align="center">Join our 'Halloween Spooktacular Pageant'</h1>
    <h3 align="center">at the McAllen Convention Center on October 29, 2016</h3>
    <hr />
    <p align="center" id="age-category-header">Age Categories</p>

    <div id="age-categories">
        <div id="girls">
            <h3>Girls</h3>
            <p>Baby Miss: 01-11 months</p>
            <p>Pee Wee Miss: 12-23 months</p>
            <p>Tiny Miss: 2-3 years</p>
            <p>Little Miss: 4-5 years</p>
            <p>Petite Miss: 6-8 years</p>
            <p>Youth Miss: 9-12 years</p>
            <p>Teen Miss: 13-15 years</p>
        </div>
        <div id="Boys">
            <h3>Boys</h3>
            <p>Baby Mr: 01-11 months</p>
            <p>Pee Wee Mr: 12-23 months</p>
            <p>Tiny Mr: 2-3 years</p>
            <p>Little Mr: 4-5 years</p>
        </div>
    </div>


</div>



<div id="about-us">
    <div id="headquarters">
        <div id="mapUpcoming" style="width:500px;height:380px; margin: 0 auto;"></div>
    </div>
</div>



